                <version>30.1.1-jre</version>
            </dependency>

            <dependency>
                <groupId>es.moki.ratelimitj</groupId>
                <artifactId>ratelimitj-inmemory</artifactId>
                <version>0.7.0</version>
            </dependency>
        </dependencies>
